{
  "breakpoint": [
    {
      "variable": "$breakpoint-sizes",
      "value": "(\n  xsmall: 320px,\n  small: 576px,\n  medium: 768px,\n  large: 992px,\n  xlarge: 1200px,\n  xxlarge: 1440px\n) !default",
      "name": "breakpoint-sizes"
    },
    {
      "variable": "$breakpoint-offset",
      "value": "1px",
      "name": "breakpoint-offset"
    }
  ],
  "flex": [
    {
      "variable": "$flex-columns",
      "value": "12 !default",
      "name": "flex-columns"
    },
    {
      "variable": "$flex-sizes",
      "value": "(\n  xsmall: breakpoint(xsmall),\n  small: breakpoint(small),\n  medium: breakpoint(medium),\n  large: breakpoint(large),\n  xlarge: breakpoint(xlarge),\n  xxlarge: breakpoint(xxlarge),\n) !default",
      "name": "flex-sizes"
    }
  ],
  "waffle": [
    {
      "variable": "$waffle-sizes",
      "value": "(\n  small: 0.3rem,\n  medium: 0.5rem,\n) !default",
      "name": "waffle-sizes"
    },
    {
      "variable": "$waffle-size",
      "value": "medium !default",
      "name": "waffle-size"
    }
  ],
  "gutter": [
    {
      "variable": "$gutter-sizes",
      "value": "(\n  small: 0.3rem,\n  medium: 0.5rem,\n) !default",
      "name": "gutter-sizes"
    },
    {
      "variable": "$gutter-size",
      "value": "medium !default",
      "name": "gutter-size"
    }
  ]
}